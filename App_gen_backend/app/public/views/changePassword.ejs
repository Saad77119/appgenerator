
<%- include("../templates/header")  %>
<div class="container row col-md-12">
    <div class="col-md-6 home-image-div" >
        <img src="/images/online-security.png" class="home_image" alt="register_image"   >
    </div>
    <div class="col-md-6 col-sm-12 col-xs-12 ">
        <div class="forgetPassword-div">
            <form id="changepass-form">
                <h1><b>Change Password</b></h1>
                <div class="form-group">
                    <label for="password">Password</label>
                    <input type="password" class="form-control" id="password" placeholder="Password">
                </div>
                <div class="form-group">
                    <label for="confirmpassword">Confirm Password</label>
                    <input type="password" class="form-control" id="confirmpassword" placeholder="Confirm Password">
                </div>
                <a class="btn btn-outline-dark w-40" href="/auth/login">Cancel</a>
                <button type="submit" class="btn btn-primary w-40">Change Password <i class="fa fa-paper-plane" aria-hidden="true"></i> </button>
                <small id="error-msg" class="text-muted text-center font-weight-bold"></small>
            </form>
        </div>
    </div>

</div>
<%- include("../templates/footer") %>
